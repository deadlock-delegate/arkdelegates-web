{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "commonjs": true,
  },
  "rules":{
    "no-console":0,
    "react/jsx-uses-vars": [2]
  },
  "extends": "eslint:recommended",
  "plugins": [
    "react"
  ]
}
